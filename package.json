{
    "name": "ticket",
    "version": "5.3.3",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "dev": "npm-run-all -p dev:*",
        "dev:client": "ng build --configuration=local",
        "dev:server": "node dist/server/ssl",
        "build": "npm run build:server && npm run build:client",
        "build:client": "ng build --configuration=production",
        "build:server": "tsc --project src/server --outDir dist/server --locale ja-jp",
        "lint": "npm-run-all -p lint:*",
        "lint:client": "tslint --config tslint.json src/client/**/**.ts --exclude **/**.spec.ts --project src/client/tsconfig.json",
        "lint:server": "tslint --config tslint.json src/server/**/**.ts --exclude **/**.spec.ts --project src/server/tsconfig.json",
        "e2e": "ng e2e",
        "start": "node dist/server/index",
        "test": "npm run lint",
        "test:client": "ng test --code-coverage",
        "preversion": "npm run build",
        "version": "git add -A",
        "postversion": "git push origin --tags",
        "clean": "rimraf src/**/*.spec.ts",
        "analyze": "webpack-bundle-analyzer dist/client/production/stats.json",
        "deploy:development": "gcloud app deploy ./app.development.gcp.yaml --project sskts-dev",
        "deploy:test": "gcloud app deploy ./app.test.gcp.yaml --project sskts-test",
        "deploy:production": "gcloud app deploy ./app.production.gcp.yaml --project sskts-prod-ticket --no-promote --version staging"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^8.2.8",
        "@angular/common": "^8.2.8",
        "@angular/compiler": "^8.2.8",
        "@angular/core": "^8.2.8",
        "@angular/forms": "^8.2.8",
        "@angular/platform-browser": "^8.2.8",
        "@angular/platform-browser-dynamic": "^8.2.8",
        "@angular/router": "^8.2.8",
        "aws-sdk": "2.538.0",
        "basic-auth": "^2.0.1",
        "bootstrap": "^4.6.0",
        "compression": "^1.7.4",
        "connect-redis": "^6.0.0",
        "cookie-parser": "^1.4.4",
        "core-js": "^3.2.1",
        "debug": "^4.1.1",
        "ejs": "^2.7.1",
        "express": "^4.17.1",
        "express-session": "^1.16.2",
        "helmet": "^3.21.1",
        "http-status": "1.3.2",
        "isomorphic-fetch": "^3.0.0",
        "libphonenumber-js": "^1.7.25",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.26",
        "ngx-bootstrap": "^5.5.0",
        "ngx-swiper-wrapper": "^8.0.2",
        "qrcode": "^1.4.2",
        "redis": "^3.1.2",
        "rxjs": "^6.5.3",
        "rxjs-compat": "^6.5.3",
        "stream": "0.0.2",
        "swiper": "^5.0.3",
        "timers": "^0.1.1",
        "uuid": "^3.3.3",
        "xml-js": "^1.6.11",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.6",
        "@angular/cli": "^8.3.6",
        "@angular/compiler-cli": "^8.2.8",
        "@angular/language-service": "^8.2.8",
        "@types/basic-auth": "^1.1.2",
        "@types/body-parser": "^1.17.1",
        "@types/compression": "^1.0.1",
        "@types/connect-redis": "0.0.11",
        "@types/cookie-parser": "^1.4.2",
        "@types/debug": "4.1.5",
        "@types/express": "^4.17.1",
        "@types/express-session": "^1.15.14",
        "@types/google.analytics": "0.0.40",
        "@types/helmet": "0.0.44",
        "@types/http-status": "^0.2.30",
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/jasmine": "^3.4.1",
        "@types/moment-timezone": "^0.5.12",
        "@types/node": "^8.10.54",
        "@types/qrcode": "^1.3.4",
        "@types/redis": "^2.8.29",
        "@types/request": "^2.48.5",
        "@types/request-promise-native": "^1.0.17",
        "@types/uuid": "^3.4.5",
        "autoprefixer": "^9.6.1",
        "codelyzer": "^5.0.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^4.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-cli": "~2.0.0",
        "karma-coverage-istanbul-reporter": "^2.1.0",
        "karma-jasmine": "^2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^7.0.18",
        "postcss-cli": "^6.1.3",
        "protractor": "~5.4.2",
        "rimraf": "^3.0.0",
        "ts-node": "~8.4.1",
        "tslint": "~5.20.0",
        "tslint-microsoft-contrib": "^6.2.0",
        "typescript": "^3.6.3",
        "webpack-bundle-analyzer": "^3.5.2"
    }
}
